{
    "name": "asha23/arlo-framework-webpack",
    "type": "project",
    "license": "MIT",
    "description": "A Composer Framework to help you get WordPress up and running in around a minute",
    "authors": [{
        "name": "Ash Whiting",
        "email": "ash_whiting@hotmail.com"
    }],
    "keywords": [
        "wordpress theme framework", "base theme", "wordpress", "composer", "webpack", "gulp","vagrant", "wp"
    ],
    "repositories": {
        "wp-packagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
		"arlo-framework-webpack-seed": {
			"type": "git",
			"url": "https://github.com/asha23/arlo-framework-webpack-seed"
		},
		"core-functionality": {
			"type": "git",
			"url": "https://github.com/asha23/Wordpress-Core-Functionality-Plugin"
		},
		"custom-gutenberg-blocks": {
			"type": "git",
			"url": "https://github.com/asha23/custom-gutenberg-blocks"
		},
		"acf": {
			"type": "package",
			"package": {
				"name": "advanced-custom-fields/advanced-custom-fields-pro",
				"version": "5.8.1",
				"type": "wordpress-plugin",
				"dist": {
				"type": "zip",
				"url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
				},
				"require": {
					"philippbaschke/acf-pro-installer": "^1.0",
					"composer/installers": "^1.0"
				}
			}
		}
		
    },
    "require": {
        "php": ">=7.1",
        "composer/installers": "~1.0.12",
        "johnpbloch/wordpress": "*",
	    "vlucas/phpdotenv": "^2.0.1",
		"oscarotero/env": "^1.0",
		"advanced-custom-fields/advanced-custom-fields-pro":"*",
		"asha23/core-functionality":"*",
		"asha23/custom-gutenberg-blocks":"*",
        "wpackagist-plugin/tinymce-advanced": "*",
        "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "*",
        "wpackagist-plugin/duplicate-post": "*",
        "wpackagist-plugin/simple-image-sizes": "*",
        "wpackagist-plugin/wordpress-seo": "*",
        "wpackagist-plugin/wps-hide-login": "*",
		"wpackagist-plugin/disable-gutenberg":"*",
		"wpackagist-plugin/svg-support": "*",
		"wpackagist-plugin/sparkpost":"*",
        "wp-sync-db/wp-sync-db": "*",
		"wpackagist-plugin/wordfence": "*",
		"asha23/arlo-framework-webpack-seed":"*"
    },
    "extra": {
        "installer-paths": {
            "web/content/plugins/{$name}/": ["type:wordpress-plugin"],
            "web/content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "web/wp",
        "content-install-dir": "web/content"
    }
}
